{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "desktop-capability",
  "windows": [
    "main"
  ],
  "platforms": [
    "linux",
    "macOS",
    "windows"
  ],
  "permissions": [
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "*://*:*/**"
        },
        {
          "url": "*://*/**"
        }
      ],
      "deny": [
        {
          "url": "https://private.tauri.app"
        }
      ]
    },
    "http:default"
  ]
}